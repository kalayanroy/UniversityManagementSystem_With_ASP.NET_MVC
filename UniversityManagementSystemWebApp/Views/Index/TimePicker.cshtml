@model UniversityManagementSystemWebApp.Models.Timepicker
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>TimePicker</title>
    <script src="~/Scripts/jquery-1.12.4.min.js"></script>
    <script src="~/Scripts/timepicker/jquery.timepicker.js"></script>
    <link href="~/Scripts/timepicker/jquery.timepicker.min.css" rel="stylesheet"/>
    <script>
        $(function() {
            $("#Time").timepicker();
        })
    </script>
</head>
<body>
<div>
    @using (Html.BeginForm(null, null, FormMethod.Post))
    {
        @Html.TextBoxFor(x=>x.Time)
        <input type="submit" value="save"/>
    }
    
</div>
</body>
</html>
